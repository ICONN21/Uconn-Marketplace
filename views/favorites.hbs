<main>
    <div class="card-panel">
    <h1>Favorites</h1>
    </div>
    {{#each favorites}}
      <div class="card-panel">
      <div class="favorite">
        <h4>{{this.item.title}}</h4>
        <p>{{this.item.description}}</p>
        <p>Posted by: {{this.item.user.email}}</p>
      </div>
      </div>
    {{/each}}
  </main>
  

